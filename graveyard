try {
            if(!this.next.equals(next)) {
                this.next = next;
                manager.updateFilesNewNode();
                manager.initialize();
            }
            if(!this.previous.equals(previous)) {
                this.previous = previous;
            }
        } catch (NullPointerException e) {
            // Neighbors are not set yet
            this.next = next;
            this.previous = previous;
        }